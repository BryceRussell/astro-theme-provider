# https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
# Set default charset
charset = utf-8
# Unix-style newlines
end_of_line = lf
# No automatic newline endings
insert_final_newline = false
# 2 character-sized tab indentation
indent_size = 2
indent_style = tab
# remove whitespace before newlines
trim_trailing_whitespace = true
